<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.login.controller.KeyEventController">
   <children>
      <BorderPane prefHeight="474.0" prefWidth="600.0">
         <center>
            <FlowPane alignment="CENTER" prefHeight="474.0" prefWidth="575.0" BorderPane.alignment="CENTER">
               <children>
                  <Pane prefHeight="282.0" prefWidth="420.0" style="-fx-border-color: black;">
                     <children>
                        <Circle fx:id="movingCircle" fill="DODGERBLUE" layoutX="210.0" layoutY="141.0" radius="23.0" stroke="BLACK" strokeType="INSIDE" />
