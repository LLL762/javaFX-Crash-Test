<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>

<AnchorPane fx:id="progressBarPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.login.controller.ProgressBarController">
   <children>
      <FlowPane alignment="CENTER" prefHeight="400.0" prefWidth="600.0">
         <children>
            <Button fx:id="button" mnemonicParsing="false" onAction="#start" text="Button" />
            <ProgressBar fx:id="progressBar" prefHeight="24.0" prefWidth="316.0" progress="0.0" />
         </children>
      </FlowPane>
   </children>
</AnchorPane>
